#--------------------------------LOGIN--------------------------
<Login@FloatLayout>
    Label:
        id:log#identificação para localização
        text:"CADERNO DE CONTAS"
        font_size:'30sp'
        color:1,1,1,1
        pos_hint: {"center_y": .8,"center_x":.5}
    Button:
        text:"Entrar"
        size_hint:.3,.1
        pos_hint: {"center_y": .2,"center_x":.675}
        on_press: root.entrar()
    Button:
        text:"Cadastrar"
        size_hint:.3,.1
        pos_hint: {"center_y": .2,"center_x":.325}
        #on_release: root.current="Cadastro"
        on_press: root.cadastrar()
    TextInput:
        id:u
        size_hint:.5,.05
        pos_hint: {"center_y": .5,"center_x":.5}
        text:"USUARIO"
    TextInput:
        id:s
        size_hint:.5,.05
        pos_hint: {"center_y": .4,"center_x":.5}
        text:"SENHA"
    Label:#textos de erro escondidos
        id:uErro
        text:"USUARIO INVALIDO"
        font_size:'17sp'
        color:0,0,0,0#color:1,0,0,1
        pos_hint: {"center_y": .55,"center_x":.5}
    Label:
        id:sErro
        text:"SENHA INVALIDA"
        font_size:'17sp'
        color:0,0,0,0#color:1,0,0,1
        pos_hint: {"center_y": .45,"center_x":.5}

